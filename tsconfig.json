{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "target": "es5",
    "outDir": "./dist/"
  },
  "include": [
    "src/**/*"
  ],

  "exclude": [
    "node_modules"
  ]
}